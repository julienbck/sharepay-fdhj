<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link rel="stylesheet" href="/css/bootstrap.min.css" />
    <link rel="stylesheet" href="/css/style.sharepay.css" />

    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">

    <title>sharepay-fdhj</title>

  </head>
  <body>
    <div class="container-sharepay">
        <div class="column1" style="background-color:#1c5170">
          <div class="logo-layout text-center"><img src="/image/LogoSharePayWhite.png" /></div>
              <div class="border-circle text-center background-avatar">
                <img src="/image/avatar.png" class="rounded-circle text-center avatar-sharepay" alt="" />
              </div>

              <p class="text-white text-center fontWelcomeUsername"> Welcome John ! </p>
          <nav class="navbar-sharepay text-center fontnav">
            <!-- Links -->
            <ul class="navbar-nav">
              <li class="nav-item">
                <a class="nav-link background-item-nav" href="/"><i class="fa fa-home" aria-hidden="true"></i> Home </a>
              </li>
              <li class="nav-item">
                <a class="nav-link background-item-nav" href="/events/new"><i class="fa fa-plus-circle" aria-hidden="true"></i> Add Event</a>
              </li>
              <li class="nav-item">
                <a class="nav-link background-item-nav" href="/events"><i class="fa fa-list" aria-hidden="true"></i> Events</a>
              </li>
              <li class="nav-item">
                <a class="nav-link background-item-nav" href="/profile"><i class="fa fa-user" aria-hidden="true"></i> Profile</a>
              </li>
            </ul>
          </nav>
          <div class="footer-navbar text-center">
            <i class="fa fa-eye-slash text-white float-l mleft25 mtop5" aria-hidden="true"></i>
            <i class="fa fa-cogs text-white" aria-hidden="true"></i>
            <a href="/logout"><i class="fa fa-sign-out text-white float-r mright25 mtop5" aria-hidden="true"></i></a>
          </div>
        </div>

      <div class="column2" style="background-color:#fff">
        <div class="header-page">
          <h3 class="text-white pt-1 text-center">{% block titlePage %}{% endblock %}
          </h3>
          <div class="content-sharepay">
          {% block content %}{% endblock %}
        </div>
        </div>



    </div>
    <!-- begin navigation -->

    <!-- end navigation -->

    <!-- begin top bar -->

    <!-- End top bar-->
  </div>

<script>
  window.fbAsyncInit = function() {
    FB.init({
      appId      : '{your-app-id}',
      cookie     : true,
      xfbml      : true,
      version    : '{latest-api-version}'
    });

    FB.AppEvents.logPageView();

  };

  (function(d, s, id){
     var js, fjs = d.getElementsByTagName(s)[0];
     if (d.getElementById(id)) {return;}
     js = d.createElement(s); js.id = id;
     js.src = "https://connect.facebook.net/en_US/sdk.js";
     fjs.parentNode.insertBefore(js, fjs);
   }(document, 'script', 'facebook-jssdk'));
</script>
  </body>
</html>
